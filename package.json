  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Panguino/MARC_Troybilt_SnowGame_site/issues"
  },
  "homepage": "https://github.com/Panguino/MARC_Troybilt_SnowGame_site#readme",
  "dependencies": {
    "normalize.scss": "^0.1.0",
    "parcel-plugin-static-files-copy": "^2.5.0"
  },
  "engines": {
    "node": "12.16.2"
  },
